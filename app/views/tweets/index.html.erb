    <div class="container">

      <div class="hero-unit">
        <h1>What's Happening?</h1>
        <br>
        <br>
      <br>
        <div class="container">
          <table id="tweets" class="table-striped table-bordered span11">
            <% @tweets.each do |tweet| %>
            <% if tweet.first_pass %>
            <tr class="tweet" data-time="<%= tweet.id %>">
              <td><h3><%= tweet.text.html_safe %></h3></td>
              <td>
                <div class="thumbnail span4">

                  <img src="<%= tweet.image %>">
                </div>
              </td>
            </tr>
            <% end %>
            <% end %>

          </table>
        </div>

        <hr>

        <footer>
          <p>&copy; Twitter Events</p>
        </footer>

      </div>
